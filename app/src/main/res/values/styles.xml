<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="whiteTextHeader">
        <item name="android:textStyle">bold</item>
        <item name="fontFamily">@font/lucida_fax_regular</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="whiteTextBody">
        <item name="android:textStyle">normal</item>
        <item name="fontFamily">@font/lucida_fax_regular</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="whiteTextContent">
        <item name="android:textStyle">normal</item>
        <item name="fontFamily">@font/lucida_fax_regular</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="blackTextHeader">
        <item name="android:textStyle">bold</item>
        <item name="fontFamily">@font/lucida_fax_demibold</item>
        <item name="android:textSize">36sp</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="blackTextBody">
        <item name="android:textStyle">bold</item>
        <item name="fontFamily">@font/lucida_fax_demibold</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="blackTextContent">
        <item name="android:textStyle">normal</item>
        <item name="fontFamily">@font/font_style_regular</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="Widget.App.Button.OutlinedButton.IconOnly" parent="Widget.Material3.Button.OutlinedButton">
        <item name="iconPadding">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:minWidth">48dp</item>
        <item name="android:minHeight">48dp</item>
    </style>
</resources>